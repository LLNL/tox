#!/bin/bash
echo -----------------------------
curl -X GET -u tester:testerpass "http://localhost:8080/tox/example.test.alive" > test1.html
curl -X GET -u tester:testerpass "http://localhost:8080/tox/example.test.formatted?in_mask=YYYYMMDD" > test2.html
curl -X GET -u tester:testerpass "http://localhost:8080/tox/example.test.formatted?in_mask=YYYYMMDD&outputType=XML" > test2.html
curl -X GET -u tester:testerpass "http://localhost:8080/tox/example.test.formatted?in_mask=YYYYMMDD&outputType=XML&xform=src/xslt/test.xsl" > test3.html
curl -X GET -u tester:testerpass "http://localhost:8080/tox/example.test.formatted?in_mask=YYYYMMDD&xform=src/xslt/test.xsl" > test4.html
curl -X GET -u tester:testerpass "http://localhost:8080/tox/example.test.formatted?in_mask=YYYYMMDD&outputType=JSON" > test5.html
curl -X POST -d "<root><date>19690720</date></root>" -u tester:testerpass "http://localhost:8080/tox/example.test.echo" > test6.html
bbedit test1.html test2.html test3.html test4.html test5.html test6.html
echo -----------------------------
